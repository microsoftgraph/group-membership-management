{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "jobsTableName": {
      "value": "syncJobs"
    },
    "serviceBusTopicSubscriptions": {
      "value": [
        {
          "name": "Organization",
          "ruleName": "syncType",
          "ruleSqlExpression": "Type = 'Organization'"
        },
        {
          "name": "SecurityGroup",
          "ruleName": "syncType",
          "ruleSqlExpression": "Type = 'SecurityGroup'"
        }
      ]
    },
    "serviceBusTopicName": {
      "value": "syncJobs"
    },
    "serviceBusQueueName": {
      "value": "membership"
    },
    "tenantId": {
      "value": "<your azure ad tenant id>"
    },
    "keyVaultReaders": {
      "value": [
        {
          "objectId": "<an azure ad group object id that should have access to this keyvault>",
          "permissions": ["get", "set", "list"]
        }
      ],
      "metadata": {
        "decription": "User(s) and/or Group(s) AAD Object Ids to which access to the keyvault will be granted to."
      }
    }    
  }
}
